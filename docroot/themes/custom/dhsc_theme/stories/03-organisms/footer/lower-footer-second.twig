{% set classes = [
  'o-footer-region-lower-second',
  'bg-coolgrey-20'
] %}
<section{{ attributes.addClass(classes) }}>
  <div class="container mx-auto w-full py-4 tablet:flex tablet:gap-4">
    {% if content is iterable  %}
      {% for item in content %}
        {{ item }}
      {% endfor %}
    {% else %}
      {{ content }}
    {% endif %}
  </div>
</section>
