<section class="o-footer o-footer--lower-first bg-coolgrey-20">
  <div class="o-footer__container container mx-auto w-full py-4 tablet:flex tablet:gap-4">
    {% if content is iterable  %}
      {% for item in content %}
        {{ item }}
      {% endfor %}
    {% else %}
      {{ content }}
    {% endif %}
  </div>
</section>
