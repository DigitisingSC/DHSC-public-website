<div class="m-tool-supplier mb-4">
  <div class="m-tool-supplier__title mb-2">
    {% if url %}
      <a href="{{ url }}" class="underline hover:no-underline">
        <h4>{{ title }}</h4>
      </a>
    {% else %}
      <h4>{{ title }}</h4>
    {% endif %}
  </div>
  <div class="m-tool-supplier__content">{{ content }}</div>
</div>
