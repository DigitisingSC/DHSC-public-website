<div id="content" role="main">
  <div class="content_align">
    <div class="core">
      <div class="sub-core">
        <section class="header">
          {% if summary %}
            <section class="-header-intro">
              <p>{{ summary }}</p>
            </section>
          {% endif %}
          <div class="section-title">
            {% if title %}
              <h1>{{ title }}</h1>
            {% endif %}
          </div>
        </section>
        <section class="app_results">
          <div class="app_section_inner">
            {% if no_result %}
              <div class="section-title">
                <h1 style="color:#000000">{{ no_result }}</h1>
              </div>
            {% else %}
              <div class="app_results_table">
                <ul>
                  {{ result }}
                </ul>
              </div>
            {% endif %}
          </div>
        </section>
      </div>
    </div>
  </div>
</div>
